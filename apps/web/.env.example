APP_NAME=OmniAI
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Configuration
DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=omni_ai
DB_USERNAME=postgres
DB_PASSWORD=

# Redis Configuration
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Session Configuration
SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=localhost

# Cache Configuration
CACHE_STORE=redis
# CACHE_PREFIX=

# Queue Configuration
QUEUE_CONNECTION=redis

# Broadcasting
BROADCAST_CONNECTION=log

# File Storage
FILESYSTEM_DISK=local

# Mail Configuration
MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@omni-ai.com"
MAIL_FROM_NAME="${APP_NAME}"

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# CDN Configuration
CDN_URL=

# Stripe Configuration
STRIPE_KEY=
STRIPE_SECRET=
STRIPE_WEBHOOK_SECRET=
STRIPE_PRODUCT_SOLO_ID=
STRIPE_PRODUCT_TEAM_ID=
STRIPE_PRICE_SOLO_MONTHLY_ID=
STRIPE_PRICE_SOLO_YEARLY_ID=
STRIPE_PRICE_TEAM_MONTHLY_ID=
STRIPE_PRICE_TEAM_YEARLY_ID=

# AI Provider API Keys
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=
MISTRAL_API_KEY=
DEEPSEEK_API_KEY=
STABILITY_API_KEY=
LEONARDO_API_KEY=
PERPLEXITY_API_KEY=
SERP_API_KEY=

# Sentry Configuration
SENTRY_LARAVEL_DSN=
SENTRY_TRACES_SAMPLE_RATE=1.0

# Vite Configuration
VITE_APP_NAME="${APP_NAME}"
